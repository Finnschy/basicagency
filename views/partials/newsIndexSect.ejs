    <% littleNewsData.forEach(news => { %>
        <div class="ftNewsFlex newsFlexLight">
            <img class="ftNewsImg" src="<%= news.img %>" alt="">
            <div class="ftNewsCopy">
                <h2 class="regularHL rosaText"><%= news.headline %></h2>
                <div>
                <p class="rosaText"><%= news.category %><span class="rosaText"><%= news.date %></span></p>
            </div>
            </div>
            <img class="arrow"src="assets/img/arrow.svg" alt="">
        </div>
    <% }) %>
